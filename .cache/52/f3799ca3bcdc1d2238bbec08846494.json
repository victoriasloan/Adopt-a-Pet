n\n        this.setState({ pets });\n      });\n  }\n\n  handleTitleClick() {\n    alert(\"you clicked the title\");\n  }\n\n  render() {\n    return (\n      <div>\n        <h1>Adopt Me!</h1>\n        <div>\n          {/* map over pet data and assign to Pet component */}\n          {this.state.pets.map(pet => {\n            let breed;\n\n            //if it has more than one breed then join the breed together\n            if (Array.isArray(pet.breeds.breed)) {\n              breed = pet.breeds.breed.join(\", \");\n            } else {\n              breed = pet.breeds.breed;\n            }\n\n            return (\n              <Pet\n                key={pet.id}\n                animal={pet.animal}\n                name={pet.name}\n                breed={breed}\n                media={pet.media}\n                location={`${pet.contact.city}, ${pet.contact.state}`}\n              />\n            );\n          })}\n        </div>\n      </div>\n    );\n  }\n}\n\nrender(React.createElement(App), document.getElementById(\"root\"));\n"},"lineCount":null}},"hash":"c5be80a6eef0557d022d68d728ea04b7","cacheData":{"env":{"API_KEY":"1d77a6e0d650060af25169c1404bdc3b","API_SECRET":"079e836b2773846dec855fd6cb4282c0"}}}